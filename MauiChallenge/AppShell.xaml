<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiChallenge.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiChallenge.Views.Pages.ContactsPage"
    Title="MauiChallenge">

    <ShellContent
        Title="Contacts"
        ContentTemplate="{DataTemplate views:ContactsPage}"
        Route="ContactPage" />
    <ShellContent 
        Title="Details" 
        Route="ContactsDetailPage" 
        ContentTemplate="{DataTemplate views:ContactsDetailPage}" />

</Shell>
